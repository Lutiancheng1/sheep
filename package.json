{
  "name": "sheep-game-monorepo",
  "version": "0.1.0",
  "private": true,
  "description": "羊了个羊游戏复刻 - Monorepo",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start": "node start.js",
